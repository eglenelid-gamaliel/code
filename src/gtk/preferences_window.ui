<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- Requirements -->
  <requires lib="gtk" version="4.0" />
  <requires lib="adw" version="1.0" />
  <requires lib="gtksource" version="5.0" />

  <!-- Window's Template -->
  <template class="PreferencesWindow" parent="AdwPreferencesWindow">

    <!-- Window's content -->
    <child>
      <object class="AdwPreferencesPage">
        <child>
          <object class="AdwPreferencesGroup" id="preferences_style_group"></object>
        </child>
      </object>
    </child>
  </template>
</interface>
